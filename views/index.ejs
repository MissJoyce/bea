	});
	//动态显示时间
	var d = new Date();
	//console.log(d);
	function date(d){
		return addZero((d.getMonth()+1))+"月"+addZero(d.getDate())+"日";
	}
	var time=date(d);
	//console.log(time);
	$(".tips p").find("span")[0].innerHTML=time;
	
	$(".foot").load("foot.html",function(){});
</script>